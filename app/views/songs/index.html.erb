<h1>Songs Index</h1>
<hr />

<%= form_tag songs_upload_path, multipart: true do %>
  <%# notice that ':file' can be any name we want. This was named :file because of the Capybara tests %>
  <%= file_field_tag :file %>
  <%= submit_tag 'Import Songs' %>
<% end %>

<ul>
<% @songs.each do |song| %>
  <li><%= display_artist(song) %> - <%= link_to song.title, song_path(song) %></li>
<% end %>
</ul>
<br>
<%= link_to "Add New Song", new_song_path %>
